{
    "name": "vinelab/neoeloquent",
    "description": "Laravel wrapper for the Neo4j graph database REST interface",
    "keywords": [
        "laravel",
        "neo4j",
        "graph",
        "database",
        "neoeloquent"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Abed Halawi",
            "email": "abed.halawi@vinelab.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "heydavid713/neo4jphp": "0.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "1.0"
    },
    "repositories": [
    ],
    "autoload": {
        "psr-4": {
            "Vinelab\\NeoEloquent\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Vinelab\\NeoEloquent\\NeoEloquentServiceProvider"
            ]
        }
    }
}
